# Login and setup your default location
az login -u <username>
az configure --defaults location=brazilsouth

# Group creation
$group="happytexts"
az group create -n $group

# Create Text Analystics service with free SKU
az cognitiveservices account create --kind textanalytics -n happytexts -g $group --sku F0

# Get the endpoint, change the position accordingly
az cognitiveservices account list --query [0].endpoint -o tsv
